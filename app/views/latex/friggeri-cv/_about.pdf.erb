\section{about}
  <% if @personal_information.street.present? %>
  <%= @personal_information.street %>
  <% end %>
  <% if @personal_information.zipCode.present? && @personal_information.town.present?%>
   <%= @personal_information.zipCode %>, <%= @personal_information.town %>
  <% elsif @personal_information.zipCode.present? %>
    <%= @personal_information.zipCode %>
  <% elsif @personal_information.town.present? %>
    <%= @personal_information.town %>
  <% end %>
  <% if @personal_information.birthday %>
  Data ur. <%= @personal_information.birthday %>
  <% end %>
  ~
  <% if @personal_information.phone.present? %>
  \faMobile\acvHeaderIconSep\<%= @personal_information.phone %>
  <% end %>
  %\href{mailto:<%= @personal_information.email %>}{\faEnvelope\acvHeaderIconSep\ <%= @personal_information.email %>}
  \href{mailto:<%= @personal_information.email %>}{<%= @personal_information.email %>}
  <% if @personal_information.homepage.present? %>
  \href{http://<%= @personal_information.homepage %>}{\faHome\acvHeaderIconSep <%= @personal_information.homepage %>}
  <% end %>
  <% if @personal_information.github.present? %>
  \href{https://github.com/<%= @personal_information.github %>}{\faGithubSquare\acvHeaderIconSep <%= @personal_information.github %>}
  <% end %>
  <% if @personal_information.linkedin.present? %>
  \href{https://www.linkedin.com/in/<%= @personal_information.linkedin %>}{\faLinkedinSquare\acvHeaderIconSep <%= @personal_information.linkedin %>}
  <% end %>
